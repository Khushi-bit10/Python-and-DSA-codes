def merge_the_tools(string, k):
    # your code goes here
    p = ""
    for i in range(len(string)):
            
        if string[i] in p:
            pass
            if (i+1)%k == 0:
                print(p)
                p=""
            
        else:
            p += string[i]
            if (i+1)%k == 0:
                print(p)
                p=""

if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
